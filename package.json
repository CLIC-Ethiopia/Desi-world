    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "three": "^0.157.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2",
    "vite-plugin-mkcert": "^1.17.1",
    "vitest": "^0.32.2"
